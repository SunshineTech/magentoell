<config>
    <modules>
        <SDM_GoogleAnalytics>
            <version>1.6.0.0</version>
        </SDM_GoogleAnalytics>
    </modules>
    <global>
        <blocks>
            <sdm_googleanalytics>
                <class>SDM_GoogleAnalytics_Block</class>
            </sdm_googleanalytics>
            <googleanalytics>
                <rewrite>
                    <ga>SDM_GoogleAnalytics_Block_Ga</ga>
                </rewrite>
            </googleanalytics>
        </blocks>
        <helpers>
            <sdm_googleanalytics>
                <class>SDM_GoogleAnalytics_Helper</class>
            </sdm_googleanalytics>
        </helpers>
    </global>
</config>
