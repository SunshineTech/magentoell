<layout>
    <adminhtml_sdm_calendar_calendar_index>
        <reference name="content">
            <block type="sdm_calendar/adminhtml_calendar" name="adminhtml.sdm.youtubefeed.calendar" />
        </reference>
    </adminhtml_sdm_calendar_calendar_index>
    <adminhtml_sdm_calendar_event_index>
        <reference name="content">
            <block type="sdm_calendar/adminhtml_event" name="adminhtml.sdm.youtubefeed.event" />
        </reference>
    </adminhtml_sdm_calendar_event_index>
    <adminhtml_sdm_calendar_event_edit>
        <update handle="editor"/>
        <reference name="head">
            <action method="setCanLoadTinyMce"><load>1</load></action>
        </reference>
    </adminhtml_sdm_calendar_event_edit>
    <adminhtml_sdm_calendar_calendar_edit>
        <update handle="editor"/>
        <reference name="head">
            <action method="setCanLoadTinyMce"><load>1</load></action>
        </reference>
    </adminhtml_sdm_calendar_calendar_edit>
</layout>